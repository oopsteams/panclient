{
  "name": "panclient",
  "productName": "panclient",
  "description": "网盘资源管理端",
  "keywords": [],
  "main": "./index.js",
  "version": "1.0.0",
  "author": "susy",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
  "dependencies": {
    "request": "2.88.0",
    "lowdb": "1.0.0",
    "jquery": "3.5.0",
    "async": "3.1.0",
    "sqlite3": "4.1.0",
    "electron-unhandled": "3.0.1"
  },
  "devDependencies": {
    "electron": "5.0.0-beta.7",
    "@electron-forge/cli": "6.0.0-beta.34",
    "@electron-forge/maker-deb": "6.0.0-beta.34",
    "@electron-forge/maker-rpm": "6.0.0-beta.34",
    "@electron-forge/maker-squirrel": "6.0.0-beta.34",
    "@electron-forge/maker-zip": "6.0.0-beta.34"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel"
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "targets": [
    {
      "target_name": "binding",
      "sources": [
        "src/binding.cc"
      ]
    }
  ]
}
